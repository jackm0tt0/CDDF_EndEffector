&ACCESS RVP
&REL 4
DEF KVP_SendPos( )

DECL E6AXIS HOME

MyBool = TRUE

BAS(#INITMOV,0)
BAS(#VEL_CP, 0.4)
BAS(#VEL_PTP, 100)
MyPos = $POS_ACT ;{E6POS: X 0, Y 0, Z 0, A 0, B 0, C 0}
HOME = {E6AXIS: A1 90, A2 -90, A3 90, A4 0, A5 0, A6 0, E1 0, E2 0}

PTP HOME

WHILE MyBool
   PTP MyPos
ENDWHILE

PTP HOME

END